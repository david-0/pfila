{
  "name": "pfila-server",
  "version": "2.0.0",
  "description": "serverpart",
  "type": "commonjs",
  "scripts": {
    "watch": "tsc -w",
    "dev": "nodemon build/app.js",
    "start:dev": "concurrently \"tsc -w\" \"nodemon build/app.js\"",
    "build": "tsc",
    "start": "ts-node src/app.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": " npm run typeorm migration:run -- -d ./src/app-data-source.ts",
    "migration:gen": " npm run typeorm migration:generate -- -d ./src/app-data-source.ts migration/schema-migration" 
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "log4js": "^6.9.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "nodemailer": "^6.9.8",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1"
  }
}
